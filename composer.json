{
    "name": "riksomers/omdb",
    "description": "OMDB Api wrapper service for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rik Somers",
            "email": "git@riksomers.nl"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "RikSomers\\OMDB\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RikSomers\\OMDB\\OMDBServiceProvider"
            ],
            "aliases": {
                "OMDB": "RikSomers\\OMDB\\Facades\\OMDBFacade"
            }
        }
    }

}
